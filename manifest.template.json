{
    "id": "{{APP_ID}}",
    "name": "{{APP_NAME}}",
    "desc": "{{APP_DESCRIPTION}}",
    "version": "{{APP_VERSION}}",
    "base_url": "https://your-ones-server.com/platform/plugin_relay/app_dispatch/{{APP_ID}}",
    "auth": {
      "type": "jwt"
    },
    "lifecycle_callback": {
      "install": "/install_cb",
      "enabled": "/enabled_cb"
    },
    "oauth": {
        "type": ["app", "user"],
        "scope": [
            "read:account:teams"
        ]
    },
    "extensions": {
        "appSettingPages": [
            {
                "key":"settingPage",
                "funcs": {
                    "customEntries": "/settingPage/entries"
                }
            }
        ]
    }
  }